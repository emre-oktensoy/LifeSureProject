@model LifeSureProject.Models.DataModels.TblContact

@{
    ViewBag.Title = "AdminContactList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="container">
    <br />
    <br />
    <h1>İletişim İçeriği</h1>
    <table class="table table-hover">
        <tr>
            <th>Adres</th>
            <th>E-posta</th>
            <th>Telefon</th>
        </tr>


        <tr>
            <td>@Model.Address</td>
            <td>@Model.Mail</td>
            <td>@Model.Telephone</td>
        </tr>

    </table>
    <a href="/AdminContact/UpdateAdminContact/@Model.ContactId" class="btn btn-primary"> Güncelle</a>
</div>


