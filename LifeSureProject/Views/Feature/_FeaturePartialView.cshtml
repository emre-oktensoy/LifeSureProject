@model LifeSureProject.Models.DataModels.TblFeature
<!-- Feature Start -->
<div class="container-fluid feature bg-light py-5">
    <div class="container py-5">
        <div class="text-center mx-auto pb-5 wow fadeInUp" data-wow-delay="0.2s" style="max-width: 800px;">
            <h4 class="text-primary">@Resources.Resource.OurFeatures</h4>
            <h1 class="display-4 mb-4">@Model.Title</h1>
            <p class="mb-0">
                @Model.Description
            </p>
        </div>
        @Html.Action("FeatureBoxPartialDefault","Feature")
    </div>
</div>
<!-- Feature End -->
