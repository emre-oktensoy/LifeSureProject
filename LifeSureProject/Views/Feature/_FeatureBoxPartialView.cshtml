@model List<LifeSureProject.Models.DataModels.TblFeatureBox>

<div class="row g-4">
    
    @foreach (var item in Model)
    {

    <div class="col-md-6 col-lg-6 col-xl-3 wow fadeInUp" data-wow-delay="0.2s">
        <div class="feature-item p-4 pt-0">
            <div class="feature-icon p-4 mb-4">
                <i class="fa @item.ImageUrl fa-3x"></i>
            </div>
            <h4 class="mb-4">@item.Title</h4>
            <p class="mb-4">
                @item.Description
            </p>
            <a class="btn btn-primary rounded-pill py-2 px-4" href="#">@Resources.Resource.ReadMore</a>
        </div>
    </div>

    }
</div>
